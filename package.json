{
  "name": "starscalendars",
  "version": "0.1.0",
  "description": "Spiritual astronomy platform with 3D visualization, WASM calculations, and Telegram integration",
  "private": true,
  "type": "module",
  "workspaces": [
    "frontend",
    "dioxus-app"
  ],
  "scripts": {
    "build": "pnpm run -r build",
    "build:frontend": "pnpm --filter frontend build",
    "build:dioxus": "pnpm --filter dioxus-app build",
    "build:wasm": "cd wasm-astro && wasm-pack build --release --target web -- --features wasm",
    "build:i18n": "pnpm run -r build:i18n",
    "dev": "pnpm run -r --parallel dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:dioxus": "pnpm --filter dioxus-app dev",
    "type-check": "pnpm run -r type-check",
    "lint": "pnpm run -r lint",
    "format": "pnpm run -r format",
    "clean": "pnpm run -r clean && rm -rf node_modules/.pnpm",
    "test": "pnpm run -r test",
    "quality-check": "make quality-check",
    "pre-commit": "make pre-commit"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "typescript": "^5.8.3",
    "vite": "^5.4.8",
    "prettier": "^3.3.3",
    "eslint": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.1",
  "keywords": [
    "astronomy",
    "astrology",
    "spiritual",
    "3d-visualization",
    "babylonjs",
    "webassembly",
    "rust",
    "telegram",
    "real-time"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/GODCRM/starscalendars.git"
  }
}
